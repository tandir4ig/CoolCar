@model EditUser

<form method="post">
    <div class="validation" asp-validation-summary="ModelOnly"></div>
    <h4>Регистрация</h4>
    <br />
    <div class="row mb-3">
        <label asp-for="UserName" class="col-sm-1 col-form-label">Логин</label>
        <div class="col-sm-3">
            <input asp-for="UserName" type="" class="form-control" >
            <span asp-validation-for="UserName" />
        </div>
    </div>
    <div class="row mb-3">
        <label asp-for="FirstName" class="col-sm-1 col-form-label">Имя</label>
        <div class="col-sm-3">
            <input asp-for="FirstName" type="" class="form-control" >
            <span asp-validation-for="FirstName" />
        </div>
    </div>
    <div class="row mb-3">
        <label asp-for="LastName" class="col-sm-1 col-form-label">Фамилия</label>
        <div class="col-sm-3">
            <input asp-for="LastName" type="" class="form-control">
            <span asp-validation-for="LastName" />
        </div>
    </div>
    <div class="row mb-3">
        <label asp-for="Phone" class="col-sm-1 col-form-label">Телефон</label>
        <div class="col-sm-3">
            <input asp-for="Phone" type="" class="form-control">
            <span asp-validation-for="Phone" />
        </div>
    </div>
    

    <button type="submit" class="btn btn-outline-primary" asp-area="Admin" asp-controller="User" asp-action="Edit" asp-route-userId="@ViewData["UserId"]">Сохранить</button>
</form>